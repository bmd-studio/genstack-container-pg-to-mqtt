{
  "name": "@bmd-studio/genstack-container-pg-to-mqtt",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "setup": "yarn install",
    "start": "NODE_OPTIONS=\"--inspect=0.0.0.0:$DEFAULT_DEBUG_PORT\" ts-node --files src/index.ts",
    "start:dev": "pm2-runtime pm2.config.js",
    "start:prod": "node dist/index.js",
    "test": "yarn test:integration",
    "test:debug": "cross-env DEBUG=pg-to-mqtt:* yarn test",
    "test:integration": "jest --runInBand --testPathPattern=integration/",
    "build": "tsc",
    "build:watch": "yarn build --watch",
    "clean": "rimraf dist/ && rimraf coverage/ && rimraf node_modules/",
    "dc": "docker-compose --env-file=./env/defaults -f docker-compose.yml --compatibility",
    "dc:dev": "yarn dc -f docker-compose.development.yml"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "change-case": "^4.1.1",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "lodash": "^4.17.14",
    "mqtt": "^3.0.0",
    "pg": "^7.11.0",
    "pg-ipc": "^1.0.4"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.157",
    "@types/pg": "^7.14.3",
    "cross-env": "^7.0.2",
    "jest": "^25.5.4",
    "pm2": "^4.4.1",
    "rimraf": "^3.0.2",
    "testcontainers": "^2.8.1",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  }
}
